# Google OAuth (required for authentication)
GOOGLE_CLIENT_ID=your-google-client-id
VITE_GOOGLE_CLIENT_ID=your-google-client-id

# TursoDB (required for database)
TURSO_DATABASE_URL=libsql://your-db.turso.io
